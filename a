[1m+++ b/vue_app/node_modules/vue-router/dist/vue-router.common.js[m
[36m@@ -0,0 +1,2625 @@[m
[32m+[m[32m/**[m
[32m+[m[32m  * vue-router v3.0.1[m
[32m+[m[32m  * (c) 2017 Evan You[m
[32m+[m[32m  * @license MIT[m
[32m+[m[32m  */[m
[32m+[m[32m'use strict';[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mfunction assert (condition, message) {[m
[32m+[m[32m  if (!condition) {[m
[32m+[m[32m    throw new Error(("[vue-router] " + message))[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction warn (condition, message) {[m
[32m+[m[32m  if (process.env.NODE_ENV !== 'production' && !condition) {[m
[32m+[m[32m    typeof console !== 'undefined' && console.warn(("[vue-router] " + message));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isError (err) {[m
[32m+[m[32m  return Object.prototype.toString.call(err).indexOf('Error') > -1[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar View = {[m
[32m+[m[32m  name: 'router-view',[m
[32m+[m[32m  functional: true,[m
[32m+[m[32m  props: {[m
[32m+[m[32m    name: {[m
[32m+[m[32m      type: String,[m
[32m+[m[32m      default: 'default'[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  render: function render (_, ref) {[m
[32m+[m[32m    var props = ref.props;[m
[32m+[m[32m    var children = ref.children;[m
[32m+[m[32m    var parent = ref.parent;[m
[32m+[m[32m    var data = ref.data;[m
[32m+[m
[32m+[m[32m    data.routerView = true;[m
[32m+[m
[32m+[m[32m    // directly use parent context's createElement() function[m
[32m+[m[32m    // so that components rendered by router-view can resolve named slots[m
[32m+[m[32m    var h = parent.$createElement;[m
[32m+[m[32m    var name = props.name;[m
[32m+[m[32m    var route = parent.$route;[m
[32m+[m[32m    var cache = parent._routerViewCache || (parent._routerViewCache = {});[m
[32m+[m
[32m+[m[32m    // determine current view depth, also check to see if the tree[m
[32m+[m[32m    // has been toggled inactive but kept-alive.[m
[32m+[m[32m    var depth = 0;[m
[32m+[m[32m    var inactive = false;[m
[32m+[m[32m    while (parent && parent._routerRoot !== parent) {[m
[32m+[m[32m      if (parent.$vnode && parent.$vnode.data.routerView) {[m
[32m+[m[32m        depth++;[m
[32m+[m[32m      }[m
[32m+[m[32m      if (parent._inactive) {[m
[32m+[m[32m        inactive = true;[m
[32m+[m[32m      }[m
[32m+[m[32m      parent = parent.$parent;[m
[32m+[m[32m    }[m
[32m+[m[32m    data.routerViewDepth = depth;[m
[32m+[m
[32m+[m[32m    // render previous view if the tree is inactive and kept-alive[m
[32m+[m[32m    if (inactive) {[m
[32m+[m[32m      return h(cache[name], data, children)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var matched = route.matched[depth];[m
[32m+[m[32m    // render empty node if no matched route[m
[32m+[m[32m    if (!matched) {[m
[32m+[m[32m      cache[name] = null;[m
[32m+[m[32m      return h()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var component = cache[name] = matched.components[name];[m
[32m+[m
[32m+[m[32m    // attach instance registration hook[m
[32m+[m[32m    // this will be called in the instance's injected lifecycle hooks[m
[32m+[m[32m    data.registerRouteInstance = function (vm, val) {[m
[32m+[m[32m      // val could be undefined for unregistration[m
[32m+[m[32m      var current = matched.instances[name];[m
[32m+[m[32m      if ([m
[32m+[m[32m        (val && current !== vm) ||[m
[32m+[m[32m        (!val && current === vm)[m
[32m+[m[32m      ) {[m
[32m+[m[32m        matched.instances[name] = val;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // also register instance in prepatch hook[m
[32m+[m[32m    // in case the same component instance is reused across different routes[m
[32m+[m[32m    ;(data.hook || (data.hook = {})).prepatch = function (_, vnode) {[m
[32m+[m[32m      matched.instances[name] = vnode.componentInstance;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    // resolve props[m
[32m+[m[32m    var propsToPass = data.props = resolveProps(route, matched.props && matched.props[name]);[m
[32m+[m[32m    if (propsToPass) {[m
[32m+[m[32m      // clone to prevent mutation[m
[32m+[m[32m      propsToPass = data.props = extend({}, propsToPass);[m
[32m+[m[32m      // pass non-declared props as attrs[m
[32m+[m[32m      var attrs = data.attrs = data.attrs || {};[m
[32m+[m[32m      for (var key in propsToPass) {[m
[32m+[m[32m        if (!component.props || !(key in component.props)) {[m
[32m+[m[32m          attrs[key] = propsToPass[key];[m
[32m+[m[32m          delete propsToPass[key];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return h(component, data, children)[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction resolveProps (route, config) {[m
[32m+[m[32m  switch (typeof config) {[m
[32m+[m[32m    case 'undefined':[m
[32m+[m[32m      return[m
[32m+[m[32m    case 'object':[m
[32m+[m[32m      return config[m
[32m+[m[32m    case 'function':[m
[32m+[m[32m      return config(route)[m
[32m+[m[32m    case 'boolean':[m
[32m+[m[32m      return config ? route.params : undefined[m
[32m+[m[32m    default:[m
[32m+[m[32m      if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m        warn([m
[32m+[m[32m          false,[m
[32m+[m[32m          "props in \"" + (route.path) + "\" is a " + (typeof config) + ", " +[m
[32m+[m[32m          "expecting an object, function or boolean."[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction extend (to, from) {[m
[32m+[m[32m  for (var key in from) {[m
[32m+[m[32m    to[key] = from[key];[m
[32m+[m[32m  }[m
[32m+[m[32m  return to[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mvar encodeReserveRE = /[!'()*]/g;[m
[32m+[m[32mvar encodeReserveReplacer = function (c) { return '%' + c.charCodeAt(0).toString(16); };[m
[32m+[m[32mvar commaRE = /%2C/g;[m
[32m+[m
[32m+[m[32m// fixed encodeURIComponent which is more conformant to RFC3986:[m
[32m+[m[32m// - escapes [!'()*][m
[32m+[m[32m// - preserve commas[m
[32m+[m[32mvar encode = function (str) { return encodeURIComponent(str)[m
[32m+[m[32m  .replace(encodeReserveRE, encodeReserveReplacer)[m
[32m+[m[32m  .replace(commaRE, ','); };[m
[32m+[m
[32m+[m[32mvar decode = decodeURIComponent;[m
[32m+[m
[32m+[m[32mfunction resolveQuery ([m
[32m+[m[32m  query,[m
[32m+[m[32m  extraQuery,[m
[32m+[m[32m  _parseQuery[m
[32m+[m[32m) {[m
[32m+[m[32m  if ( extraQuery === void 0 ) extraQuery = {};[m
[32m+[m
[32m+[m[32m  var parse = _parseQuery || parseQuery;[m
[32m+[m[32m  var parsedQuery;[m
[32m+[m[32m  try {[m
[32m+[m[32m    parsedQuery = parse(query || '');[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    process.env.NODE_ENV !== 'production' && warn(false, e.message);[m
[32m+[m[32m    parsedQuery = {};[m
[32m+[m[32m  }[m
[32m+[m[32m  for (var key in extraQuery) {[m
[32m+[m[32m    parsedQuery[key] = extraQuery[key];[m
[32m+[m[32m  }[m
[32m+[m[32m  return parsedQuery[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction parseQuery (query) {[m
[32m+[m[32m  var res = {};[m
[32m+[m
[32m+[m[32m  query = query.trim().replace(/^(\?|#|&)/, '');[m
[32m+[m
[32m+[m[32m  if (!query) {[m
[32m+[m[32m    return res[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  query.split('&').forEach(function (param) {[m
[32m+[m[32m    var parts = param.replace(/\+/g, ' ').split('=');[m
[32m+[m[32m    var key = decode(parts.shift());[m
[32m+[m[32m    var val = parts.length > 0[m
[32m+[m[32m      ? decode(parts.join('='))[m
[32m+[m[32m      : null;[m
[32m+[m
[32m+[m[32m    if (res[key] === undefined) {[m
[32m+[m[32m      res[key] = val;[m
[32m+[m[32m    } else if (Array.isArray(res[key])) {[m
[32m+[m[32m      res[key].push(val);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      res[key] = [res[key], val];[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  return res[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction stringifyQuery (obj) {[m
[32m+[m[32m  var res = obj ? Object.keys(obj).map(function (key) {[m
[32m+[m[32m    var val = obj[key];[m
[32m+[m
[32m+[m[32m    if (val === undefined) {[m
[32m+[m[32m      return ''[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (val === null) {[m
[32m+[m[32m      return encode(key)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (Array.isArray(val)) {[m
[32m+[m[32m      var result = [];[m
[32m+[m[32m      val.forEach(function (val2) {[m
[32m+[m[32m        if (val2 === undefined) {[m
[32m+[m[32m          return[m
[32m+[m[32m        }[m
[32m+[m[32m        if (val2 === null) {[m
[32m+[m[32m          result.push(encode(key));[m
[32m+[m[32m        } else {[m
[32m+[m[32m          result.push(encode(key) + '=' + encode(val2));[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m      return result.join('&')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return encode(key) + '=' + encode(val)[m
[32m+[m[32m  }).filter(function (x) { return x.length > 0; }).join('&') : null;[m
[32m+[m[32m  return res ? ("?" + res) : ''[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m
[32m+[m[32mvar trailingSlashRE = /\/?$/;[m
[32m+[m
[32m+[m[32mfunction createRoute ([m
[32m+[m[32m  record,[m
[32m+[m[32m  location,[m
[32m+[m[32m  redirectedFrom,[m
[32m+[m[32m  router[m
[32m+[m[32m) {[m
[32m+[m[32m  var stringifyQuery$$1 = router && router.options.stringifyQuery;[m
[32m+[m
[32m+[m[32m  var query = location.query || {};[m
[32m+[m[32m  try {[m
[32m+[m[32m    query = clone(query);[m
[32m+[m[32m  } catch (e) {}[m
[32m+[m
[32m+[m[32m  var route = {[m
[32m+[m[32m    name: location.name || (record && record.name),[m
[32m+[m[32m    meta: (record && record.meta) || {},[m
[32m+[m[32m    path: location.path || '/',[m
[32m+[m[32m    hash: location.hash || '',[m
[32m+[m[32m    query: query,[m
[32m+[m[32m    params: location.params || {},[m
[32m+[m[32m    fullPath: getFullPath(location, stringifyQuery$$1),[m
[32m+[m[32m    matched: record ? formatMatch(record) : [][m
[32m+[m[32m  };[m
[32m+[m[32m  if (redirectedFrom) {[m
[32m+[m[32m    route.redirectedFrom = getFullPath(redirectedFrom, stringifyQuery$$1);[m
[32m+[m[32m  }[m
[32m+[m[32m  return Object.freeze(route)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction clone (value) {[m
[32m+[m[32m  if (Array.isArray(value)) {[m
[32m+[m[32m    return value.map(clone)[m
[32m+[m[32m  } else if (value && typeof value === 'object') {[m
[32m+[m[32m    var res = {};[m
[32m+[m[32m    for (var key in value) {[m
[32m+[m[32m      res[key] = clone(value[key]);[m
[32m+[m[32m    }[m
[32m+[m[32m    return res[m
[32m+[m[32m  } else {[m
[32m+[m[32m    return value[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// the starting route that represents the initial state[m
[32m+[m[32mvar START = createRoute(null, {[m
[32m+[m[32m  path: '/'[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mfunction formatMatch (record) {[m
[32m+[m[32m  var res = [];[m
[32m+[m[32m  while (record) {[m
[32m+[m[32m    res.unshift(record);[m
[32m+[m[32m    record = record.parent;[m
[32m+[m[32m  }[m
[32m+[m[32m  return res[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getFullPath ([m
[32m+[m[32m  ref,[m
[32m+[m[32m  _stringifyQuery[m
[32m+[m[32m) {[m
[32m+[m[32m  var path = ref.path;[m
[32m+[m[32m  var query = ref.query; if ( query === void 0 ) query = {};[m
[32m+[m[32m  var hash = ref.hash; if ( hash === void 0 ) hash = '';[m
[32m+[m
[32m+[m[32m  var stringify = _stringifyQuery || stringifyQuery;[m
[32m+[m[32m  return (path || '/') + stringify(query) + hash[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isSameRoute (a, b) {[m
[32m+[m[32m  if (b === START) {[m
[32m+[m[32m    return a === b[m
[32m+[m[32m  } else if (!b) {[m
[32m+[m[32m    return false[m
[32m+[m[32m  } else if (a.path && b.path) {[m
[32m+[m[32m    return ([m
[32m+[m[32m      a.path.replace(trailingSlashRE, '') === b.path.replace(trailingSlashRE, '') &&[m
[32m+[m[32m      a.hash === b.hash &&[m
[32m+[m[32m      isObjectEqual(a.query, b.query)[m
[32m+[m[32m    )[m
[32m+[m[32m  } else if (a.name && b.name) {[m
[32m+[m[32m    return ([m
[32m+[m[32m      a.name === b.name &&[m
[32m+[m[32m      a.hash === b.hash &&[m
[32m+[m[32m      isObjectEqual(a.query, b.query) &&[m
[32m+[m[32m      isObjectEqual(a.params, b.params)[m
[32m+[m[32m    )[m
[32m+[m[32m  } else {[m
[32m+[m[32m    return false[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isObjectEqual (a, b) {[m
[32m+[m[32m  if ( a === void 0 ) a = {};[m
[32m+[m[32m  if ( b === void 0 ) b = {};[m
[32m+[m
[32m+[m[32m  // handle null value #1566[m
[32m+[m[32m  if (!a || !b) { return a === b }[m
[32m+[m[32m  var aKeys = Object.keys(a);[m
[32m+[m[32m  var bKeys = Object.keys(b);[m
[32m+[m[32m  if (aKeys.length !== bKeys.length) {[m
[32m+[m[32m    return false[m
[32m+[m[32m  }[m
[32m+[m[32m  return aKeys.every(function (key) {[m
[32m+[m[32m    var aVal = a[key];[m
[32m+[m[32m    var bVal = b[key];[m
[32m+[m[32m    // check nested equality[m
[32m+[m[32m    if (typeof aVal === 'object' && typeof bVal === 'object') {[m
[32m+[m[32m      return isObjectEqual(aVal, bVal)[m
[32m+[m[32m    }[m
[32m+[m[32m    return String(aVal) === String(bVal)[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isIncludedRoute (current, target) {[m
[32m+[m[32m  return ([m
[32m+[m[32m    current.path.replace(trailingSlashRE, '/').indexOf([m
[32m+[m[32m      target.path.replace(trailingSlashRE, '/')[m
[32m+[m[32m    ) === 0 &&[m
[32m+[m[32m    (!target.hash || current.hash === target.hash) &&[m
[32m+[m[32m    queryIncludes(current.query, target.query)[m
[32m+[m[32m  )[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction queryIncludes (current, target) {[m
[32m+[m[32m  for (var key in target) {[m
[32m+[m[32m    if (!(key in current)) {[m
[32m+[m[32m      return false[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return true[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32m// work around weird flow bug[m
[32m+[m[32mvar toTypes = [String, Object];[m
[32m+[m[32mvar eventTypes = [String, Array];[m
[32m+[m
[32m+[m[32mvar Link = {[m
[32m+[m[32m  name: 'router-link',[m
[32m+[m[32m  props: {[m
[32m+[m[32m    to: {[m
[32m+[m[32m      type: toTypes,[m
[32m+[m[32m      required: true[m
[32m+[m[32m    },[m
[32m+[m[32m    tag: {[m
[32m+[m[32m      type: String,[m
[32m+[m[32m      default: 'a'[m
[32m+[m[32m    },[m
[32m+[m[32m    exact: Boolean,[m
[32m+[m[32m    append: Boolean,[m
[32m+[m[32m    replace: Boolean,[m
[32m+[m[32m    activeClass: String,[m
[32m+[m[32m    exactActiveClass: String,[m
[32m+[m[32m    event: {[m
[32m+[m[32m      type: eventTypes,[m
[32m+[m[32m      default: 'click'[m
[32m+[m[32m    }[m
[32m+[m[32m  },[m
[32m+[m[32m  render: function render (h) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    var router = this.$router;[m
[32m+[m[32m    var current = this.$route;[m
[32m+[m[32m    var ref = router.resolve(this.to, current, this.append);[m
[32m+[m[32m    var location = ref.location;[m
[32m+[m[32m    var route = ref.route;[m
[32m+[m[32m    var href = ref.href;[m
[32m+[m
[32m+[m[32m    var classes = {};[m
[32m+[m[32m    var globalActiveClass = router.options.linkActiveClass;[m
[32m+[m[32m    var globalExactActiveClass = router.options.linkExactActiveClass;[m
[32m+[m[32m    // Support global empty active class[m
[32m+[m[32m    var activeClassFallback = globalActiveClass == null[m
[32m+[m[32m            ? 'router-link-active'[m
[32m+[m[32m            : globalActiveClass;[m
[32m+[m[32m    var exactActiveClassFallback = globalExactActiveClass == null[m
[32m+[m[32m            ? 'router-link-exact-active'[m
[32m+[m[32m            : globalExactActiveClass;[m
[32m+[m[32m    var activeClass = this.activeClass == null[m
[32m+[m[32m            ? activeClassFallback[m
[32m+[m[32m            : this.activeClass;[m
[32m+[m[32m    var exactActiveClass = this.exactActiveClass == null[m
[32m+[m[32m            ? exactActiveClassFallback[m
[32m+[m[32m            : this.exactActiveClass;[m
[32m+[m[32m    var compareTarget = location.path[m
[32m+[m[32m      ? createRoute(null, location, null, router)[m
[32m+[m[32m      : route;[m
[32m+[m
[32m+[m[32m    classes[exactActiveClass] = isSameRoute(current, compareTarget);[m
[32m+[m[32m    classes[activeClass] = this.exact[m
[32m+[m[32m      ? classes[exactActiveClass][m
[32m+[m[32m      : isIncludedRoute(current, compareTarget);[m
[32m+[m
[32m+[m[32m    var handler = function (e) {[m
[32m+[m[32m      if (guardEvent(e)) {[m
[32m+[m[32m        if (this$1.replace) {[m
[32m+[m[32m          router.replace(location);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          router.push(location);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var on = { click: guardEvent };[m
[32m+[m[32m    if (Array.isArray(this.event)) {[m
[32m+[m[32m      this.event.forEach(function (e) { on[e] = handler; });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      on[this.event] = handler;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var data = {[m
[32m+[m[32m      class: classes[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    if (this.tag === 'a') {[m
[32m+[m[32m      data.on = on;[m
[32m+[m[32m      data.attrs = { href: href };[m
[32m+[m[32m    } else {[m
[32m+[m[32m      // find the first <a> child and apply listener and href[m
[32m+[m[32m      var a = findAnchor(this.$slots.default);[m
[32m+[m[32m      if (a) {[m
[32m+[m[32m        // in case the <a> is a static node[m
[32m+[m[32m        a.isStatic = false;[m
[32m+[m[32m        var extend = _Vue.util.extend;[m
[32m+[m[32m        var aData = a.data = extend({}, a.data);[m
[32m+[m[32m        aData.on = on;[m
[32m+[m[32m        var aAttrs = a.data.attrs = extend({}, a.data.attrs);[m
[32m+[m[32m        aAttrs.href = href;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        // doesn't have <a> child, apply listener to self[m
[32m+[m[32m        data.on = on;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return h(this.tag, data, this.$slots.default)[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction guardEvent (e) {[m
[32m+[m[32m  // don't redirect with control keys[m
[32m+[m[32m  if (e.metaKey || e.altKey || e.ctrlKey || e.shiftKey) { return }[m
[32m+[m[32m  // don't redirect when preventDefault called[m
[32m+[m[32m  if (e.defaultPrevented) { return }[m
[32m+[m[32m  // don't redirect on right click[m
[32m+[m[32m  if (e.button !== undefined && e.button !== 0) { return }[m
[32m+[m[32m  // don't redirect if `target="_blank"`[m
[32m+[m[32m  if (e.currentTarget && e.currentTarget.getAttribute) {[m
[32m+[m[32m    var target = e.currentTarget.getAttribute('target');[m
[32m+[m[32m    if (/\b_blank\b/i.test(target)) { return }[m
[32m+[m[32m  }[m
[32m+[m[32m  // this may be a Weex event which doesn't have this method[m
[32m+[m[32m  if (e.preventDefault) {[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[32m  }[m
[32m+[m[32m  return true[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction findAnchor (children) {[m
[32m+[m[32m  if (children) {[m
[32m+[m[32m    var child;[m
[32m+[m[32m    for (var i = 0; i < children.length; i++) {[m
[32m+[m[32m      child = children[i];[m
[32m+[m[32m      if (child.tag === 'a') {[m
[32m+[m[32m        return child[m
[32m+[m[32m      }[m
[32m+[m[32m      if (child.children && (child = findAnchor(child.children))) {[m
[32m+[m[32m        return child[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar _Vue;[m
[32m+[m
[32m+[m[32mfunction install (Vue) {[m
[32m+[m[32m  if (install.installed && _Vue === Vue) { return }[m
[32m+[m[32m  install.installed = true;[m
[32m+[m
[32m+[m[32m  _Vue = Vue;[m
[32m+[m
[32m+[m[32m  var isDef = function (v) { return v !== undefined; };[m
[32m+[m
[32m+[m[32m  var registerInstance = function (vm, callVal) {[m
[32m+[m[32m    var i = vm.$options._parentVnode;[m
[32m+[m[32m    if (isDef(i) && isDef(i = i.data) && isDef(i = i.registerRouteInstance)) {[m
[32m+[m[32m      i(vm, callVal);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  Vue.mixin({[m
[32m+[m[32m    beforeCreate: function beforeCreate () {[m
[32m+[m[32m      if (isDef(this.$options.router)) {[m
[32m+[m[32m        this._routerRoot = this;[m
[32m+[m[32m        this._router = this.$options.router;[m
[32m+[m[32m        this._router.init(this);[m
[32m+[m[32m        Vue.util.defineReactive(this, '_route', this._router.history.current);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this._routerRoot = (this.$parent && this.$parent._routerRoot) || this;[m
[32m+[m[32m      }[m
[32m+[m[32m      registerInstance(this, this);[m
[32m+[m[32m    },[m
[32m+[m[32m    destroyed: function destroyed () {[m
[32m+[m[32m      registerInstance(this);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  Object.defineProperty(Vue.prototype, '$router', {[m
[32m+[m[32m    get: function get () { return this._routerRoot._router }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  Object.defineProperty(Vue.prototype, '$route', {[m
[32m+[m[32m    get: function get () { return this._routerRoot._route }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  Vue.component('router-view', View);[m
[32m+[m[32m  Vue.component('router-link', Link);[m
[32m+[m
[32m+[m[32m  var strats = Vue.config.optionMergeStrategies;[m
[32m+[m[32m  // use the same hook merging strategy for route hooks[m
[32m+[m[32m  strats.beforeRouteEnter = strats.beforeRouteLeave = strats.beforeRouteUpdate = strats.created;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mvar inBrowser = typeof window !== 'undefined';[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mfunction resolvePath ([m
[32m+[m[32m  relative,[m
[32m+[m[32m  base,[m
[32m+[m[32m  append[m
[32m+[m[32m) {[m
[32m+[m[32m  var firstChar = relative.charAt(0);[m
[32m+[m[32m  if (firstChar === '/') {[m
[32m+[m[32m    return relative[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (firstChar === '?' || firstChar === '#') {[m
[32m+[m[32m    return base + relative[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var stack = base.split('/');[m
[32m+[m
[32m+[m[32m  // remove trailing segment if:[m
[32m+[m[32m  // - not appending[m
[32m+[m[32m  // - appending to trailing slash (last segment is empty)[m
[32m+[m[32m  if (!append || !stack[stack.length - 1]) {[m
[32m+[m[32m    stack.pop();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // resolve relative path[m
[32m+[m[32m  var segments = relative.replace(/^\//, '').split('/');[m
[32m+[m[32m  for (var i = 0; i < segments.length; i++) {[m
[32m+[m[32m    var segment = segments[i];[m
[32m+[m[32m    if (segment === '..') {[m
[32m+[m[32m      stack.pop();[m
[32m+[m[32m    } else if (segment !== '.') {[m
[32m+[m[32m      stack.push(segment);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // ensure leading slash[m
[32m+[m[32m  if (stack[0] !== '') {[m
[32m+[m[32m    stack.unshift('');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return stack.join('/')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction parsePath (path) {[m
[32m+[m[32m  var hash = '';[m
[32m+[m[32m  var query = '';[m
[32m+[m
[32m+[m[32m  var hashIndex = path.indexOf('#');[m
[32m+[m[32m  if (hashIndex >= 0) {[m
[32m+[m[32m    hash = path.slice(hashIndex);[m
[32m+[m[32m    path = path.slice(0, hashIndex);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var queryIndex = path.indexOf('?');[m
[32m+[m[32m  if (queryIndex >= 0) {[m
[32m+[m[32m    query = path.slice(queryIndex + 1);[m
[32m+[m[32m    path = path.slice(0, queryIndex);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    path: path,[m
[32m+[m[32m    query: query,[m
[32m+[m[32m    hash: hash[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction cleanPath (path) {[m
[32m+[m[32m  return path.replace(/\/\//g, '/')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar isarray = Array.isArray || function (arr) {[m
[32m+[m[32m  return Object.prototype.toString.call(arr) == '[object Array]';[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose `pathToRegexp`.[m
[32m+[m[32m */[m
[32m+[m[32mvar pathToRegexp_1 = pathToRegexp;[m
[32m+[m[32mvar parse_1 = parse;[m
[32m+[m[32mvar compile_1 = compile;[m
[32m+[m[32mvar tokensToFunction_1 = tokensToFunction;[m
[32m+[m[32mvar tokensToRegExp_1 = tokensToRegExp;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * The main path matching regexp utility.[m
[32m+[m[32m *[m
[32m+[m[32m * @type {RegExp}[m
[32m+[m[32m */[m
[32m+[m[32mvar PATH_REGEXP = new RegExp([[m
[32m+[m[32m  // Match escaped characters that would otherwise appear in future matches.[m
[32m+[m[32m  // This allows the user to escape special characters that won't transform.[m
[32m+[m[32m  '(\\\\.)',[m
[32m+[m[32m  // Match Express-style parameters and un-named parameters with a prefix[m
[32m+[m[32m  // and optional suffixes. Matches appear as:[m
[32m+[m[32m  //[m
[32m+[m[32m  // "/:test(\\d+)?" => ["/", "test", "\d+", undefined, "?", undefined][m
[32m+[m[32m  // "/route(\\d+)"  => [undefined, undefined, undefined, "\d+", undefined, undefined][m
[32m+[m[32m  // "/*"            => ["/", undefined, undefined, undefined, undefined, "*"][m
[32m+[m[32m  '([\\/.])?(?:(?:\\:(\\w+)(?:\\(((?:\\\\.|[^\\\\()])+)\\))?|\\(((?:\\\\.|[^\\\\()])+)\\))([+*?])?|(\\*))'[m
[32m+[m[32m].join('|'), 'g');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Parse a string for the raw tokens.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {string}  str[m
[32m+[m[32m * @param  {Object=} options[m
[32m+[m[32m * @return {!Array}[m
[32m+[m[32m */[m
[32m+[m[32mfunction parse (str, options) {[m
[32m+[m[32m  var tokens = [];[m
[32m+[m[32m  var key = 0;[m
[32m+[m[32m  var index = 0;[m
[32m+[m[32m  var path = '';[m
[32m+[m[32m  var defaultDelimiter = options && options.delimiter || '/';[m
[32m+[m[32m  var res;[m
[32m+[m
[32m+[m[32m  while ((res = PATH_REGEXP.exec(str)) != null) {[m
[32m+[m[32m    var m = res[0];[m
[32m+[m[32m    var escaped = res[1];[m
[32m+[m[32m    var offset = res.index;[m
[32m+[m[32m    path += str.slice(index, offset);[m
[32m+[m[32m    index = offset + m.length;[m
[32m+[m
[32m+[m[32m    // Ignore already escaped sequences.[m
[32m+[m[32m    if (escaped) {[m
[32m+[m[32m      path += escaped[1];[m
[32m+[m[32m      continue[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var next = str[index];[m
[32m+[m[32m    var prefix = res[2];[m
[32m+[m[32m    var name = res[3];[m
[32m+[m[32m    var capture = res[4];[m
[32m+[m[32m    var group = res[5];[m
[32m+[m[32m    var modifier = res[6];[m
[32m+[m[32m    var asterisk = res[7];[m
[32m+[m
[32m+[m[32m    // Push the current path onto the tokens.[m
[32m+[m[32m    if (path) {[m
[32m+[m[32m      tokens.push(path);[m
[32m+[m[32m      path = '';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var partial = prefix != null && next != null && next !== prefix;[m
[32m+[m[32m    var repeat = modifier === '+' || modifier === '*';[m
[32m+[m[32m    var optional = modifier === '?' || modifier === '*';[m
[32m+[m[32m    var delimiter = res[2] || defaultDelimiter;[m
[32m+[m[32m    var pattern = capture || group;[m
[32m+[m
[32m+[m[32m    tokens.push({[m
[32m+[m[32m      name: name || key++,[m
[32m+[m[32m      prefix: prefix || '',[m
[32m+[m[32m      delimiter: delimiter,[m
[32m+[m[32m      optional: optional,[m
[32m+[m[32m      repeat: repeat,[m
[32m+[m[32m      partial: partial,[m
[32m+[m[32m      asterisk: !!asterisk,[m
[32m+[m[32m      pattern: pattern ? escapeGroup(pattern) : (asterisk ? '.*' : '[^' + escapeString(delimiter) + ']+?')[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Match any characters still remaining.[m
[32m+[m[32m  if (index < str.length) {[m
[32m+[m[32m    path += str.substr(index);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // If the path exists, push it onto the end.[m
[32m+[m[32m  if (path) {[m
[32m+[m[32m    tokens.push(path);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return tokens[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Compile a string to a template function for the path.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {string}             str[m
[32m+[m[32m * @param  {Object=}            options[m
[32m+[m[32m * @return {!function(Object=, Object=)}[m
[32m+[m[32m */[m
[32m+[m[32mfunction compile (str, options) {[m
[32m+[m[32m  return tokensToFunction(parse(str, options))[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Prettier encoding of URI path segments.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {string}[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m */[m
[32m+[m[32mfunction encodeURIComponentPretty (str) {[m
[32m+[m[32m  return encodeURI(str).replace(/[\/?#]/g, function (c) {[m
[32m+[m[32m    return '%' + c.charCodeAt(0).toString(16).toUpperCase()[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Encode the asterisk parameter. Similar to `pretty`, but allows slashes.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {string}[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m */[m
[32m+[m[32mfunction encodeAsterisk (str) {[m
[32m+[m[32m  return encodeURI(str).replace(/[?#]/g, function (c) {[m
[32m+[m[32m    return '%' + c.charCodeAt(0).toString(16).toUpperCase()[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose a method for transforming tokens into the path function.[m
[32m+[m[32m */[m
[32m+[m[32mfunction tokensToFunction (tokens) {[m
[32m+[m[32m  // Compile all the tokens into regexps.[m
[32m+[m[32m  var matches = new Array(tokens.length);[m
[32m+[m
[32m+[m[32m  // Compile all the patterns before compilation.[m
[32m+[m[32m  for (var i = 0; i < tokens.length; i++) {[m
[32m+[m[32m    if (typeof tokens[i] === 'object') {[m
[32m+[m[32m      matches[i] = new RegExp('^(?:' + tokens[i].pattern + ')$');[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return function (obj, opts) {[m
[32m+[m[32m    var path = '';[m
[32m+[m[32m    var data = obj || {};[m
[32m+[m[32m    var options = opts || {};[m
[32m+[m[32m    var encode = options.pretty ? encodeURIComponentPretty : encodeURIComponent;[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < tokens.length; i++) {[m
[32m+[m[32m      var token = tokens[i];[m
[32m+[m
[32m+[m[32m      if (typeof token === 'string') {[m
[32m+[m[32m        path += token;[m
[32m+[m
[32m+[m[32m        continue[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var value = data[token.name];[m
[32m+[m[32m      var segment;[m
[32m+[m
[32m+[m[32m      if (value == null) {[m
[32m+[m[32m        if (token.optional) {[m
[32m+[m[32m          // Prepend partial segment prefixes.[m
[32m+[m[32m          if (token.partial) {[m
[32m+[m[32m            path += token.prefix;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          continue[m
[32m+[m[32m        } else {[m
[32m+[m[32m          throw new TypeError('Expected "' + token.name + '" to be defined')[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (isarray(value)) {[m
[32m+[m[32m        if (!token.repeat) {[m
[32m+[m[32m          throw new TypeError('Expected "' + token.name + '" to not repeat, but received `' + JSON.stringify(value) + '`')[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (value.length === 0) {[m
[32m+[m[32m          if (token.optional) {[m
[32m+[m[32m            continue[m
[32m+[m[32m          } else {[m
[32m+[m[32m            throw new TypeError('Expected "' + token.name + '" to not be empty')[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        for (var j = 0; j < value.length; j++) {[m
[32m+[m[32m          segment = encode(value[j]);[m
[32m+[m
[32m+[m[32m          if (!matches[i].test(segment)) {[m
[32m+[m[32m            throw new TypeError('Expected all "' + token.name + '" to match "' + token.pattern + '", but received `' + JSON.stringify(segment) + '`')[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          path += (j === 0 ? token.prefix : token.delimiter) + segment;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        continue[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      segment = token.asterisk ? encodeAsterisk(value) : encode(value);[m
[32m+[m
[32m+[m[32m      if (!matches[i].test(segment)) {[m
[32m+[m[32m        throw new TypeError('Expected "' + token.name + '" to match "' + token.pattern + '", but received "' + segment + '"')[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      path += token.prefix + segment;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return path[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Escape a regular expression string.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {string} str[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m */[m
[32m+[m[32mfunction escapeString (str) {[m
[32m+[m[32m  return str.replace(/([.+*?=^!:${}()[\]|\/\\])/g, '\\$1')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Escape the capturing group by escaping special characters and meaning.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {string} group[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m */[m
[32m+[m[32mfunction escapeGroup (group) {[m
[32m+[m[32m  return group.replace(/([=!:$\/()])/g, '\\$1')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Attach the keys as a property of the regexp.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {!RegExp} re[m
[32m+[m[32m * @param  {Array}   keys[m
[32m+[m[32m * @return {!RegExp}[m
[32m+[m[32m */[m
[32m+[m[32mfunction attachKeys (re, keys) {[m
[32m+[m[32m  re.keys = keys;[m
[32m+[m[32m  return re[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Get the flags for a regexp from the options.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {Object} options[m
[32m+[m[32m * @return {string}[m
[32m+[m[32m */[m
[32m+[m[32mfunction flags (options) {[m
[32m+[m[32m  return options.sensitive ? '' : 'i'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Pull out keys from a regexp.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {!RegExp} path[m
[32m+[m[32m * @param  {!Array}  keys[m
[32m+[m[32m * @return {!RegExp}[m
[32m+[m[32m */[m
[32m+[m[32mfunction regexpToRegexp (path, keys) {[m
[32m+[m[32m  // Use a negative lookahead to match only capturing groups.[m
[32m+[m[32m  var groups = path.source.match(/\((?!\?)/g);[m
[32m+[m
[32m+[m[32m  if (groups) {[m
[32m+[m[32m    for (var i = 0; i < groups.length; i++) {[m
[32m+[m[32m      keys.push({[m
[32m+[m[32m        name: i,[m
[32m+[m[32m        prefix: null,[m
[32m+[m[32m        delimiter: null,[m
[32m+[m[32m        optional: false,[m
[32m+[m[32m        repeat: false,[m
[32m+[m[32m        partial: false,[m
[32m+[m[32m        asterisk: false,[m
[32m+[m[32m        pattern: null[m
[32m+[m[32m      });[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return attachKeys(path, keys)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Transform an array into a regexp.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {!Array}  path[m
[32m+[m[32m * @param  {Array}   keys[m
[32m+[m[32m * @param  {!Object} options[m
[32m+[m[32m * @return {!RegExp}[m
[32m+[m[32m */[m
[32m+[m[32mfunction arrayToRegexp (path, keys, options) {[m
[32m+[m[32m  var parts = [];[m
[32m+[m
[32m+[m[32m  for (var i = 0; i < path.length; i++) {[m
[32m+[m[32m    parts.push(pathToRegexp(path[i], keys, options).source);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var regexp = new RegExp('(?:' + parts.join('|') + ')', flags(options));[m
[32m+[m
[32m+[m[32m  return attachKeys(regexp, keys)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create a path regexp from string input.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {string}  path[m
[32m+[m[32m * @param  {!Array}  keys[m
[32m+[m[32m * @param  {!Object} options[m
[32m+[m[32m * @return {!RegExp}[m
[32m+[m[32m */[m
[32m+[m[32mfunction stringToRegexp (path, keys, options) {[m
[32m+[m[32m  return tokensToRegExp(parse(path, options), keys, options)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Expose a function for taking tokens and returning a RegExp.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {!Array}          tokens[m
[32m+[m[32m * @param  {(Array|Object)=} keys[m
[32m+[m[32m * @param  {Object=}         options[m
[32m+[m[32m * @return {!RegExp}[m
[32m+[m[32m */[m
[32m+[m[32mfunction tokensToRegExp (tokens, keys, options) {[m
[32m+[m[32m  if (!isarray(keys)) {[m
[32m+[m[32m    options = /** @type {!Object} */ (keys || options);[m
[32m+[m[32m    keys = [];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  options = options || {};[m
[32m+[m
[32m+[m[32m  var strict = options.strict;[m
[32m+[m[32m  var end = options.end !== false;[m
[32m+[m[32m  var route = '';[m
[32m+[m
[32m+[m[32m  // Iterate over the tokens and create our regexp string.[m
[32m+[m[32m  for (var i = 0; i < tokens.length; i++) {[m
[32m+[m[32m    var token = tokens[i];[m
[32m+[m
[32m+[m[32m    if (typeof token === 'string') {[m
[32m+[m[32m      route += escapeString(token);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      var prefix = escapeString(token.prefix);[m
[32m+[m[32m      var capture = '(?:' + token.pattern + ')';[m
[32m+[m
[32m+[m[32m      keys.push(token);[m
[32m+[m
[32m+[m[32m      if (token.repeat) {[m
[32m+[m[32m        capture += '(?:' + prefix + capture + ')*';[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (token.optional) {[m
[32m+[m[32m        if (!token.partial) {[m
[32m+[m[32m          capture = '(?:' + prefix + '(' + capture + '))?';[m
[32m+[m[32m        } else {[m
[32m+[m[32m          capture = prefix + '(' + capture + ')?';[m
[32m+[m[32m        }[m
[32m+[m[32m      } else {[m
[32m+[m[32m        capture = prefix + '(' + capture + ')';[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      route += capture;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var delimiter = escapeString(options.delimiter || '/');[m
[32m+[m[32m  var endsWithDelimiter = route.slice(-delimiter.length) === delimiter;[m
[32m+[m
[32m+[m[32m  // In non-strict mode we allow a slash at the end of match. If the path to[m
[32m+[m[32m  // match already ends with a slash, we remove it for consistency. The slash[m
[32m+[m[32m  // is valid at the end of a path match, not in the middle. This is important[m
[32m+[m[32m  // in non-ending mode, where "/test/" shouldn't match "/test//route".[m
[32m+[m[32m  if (!strict) {[m
[32m+[m[32m    route = (endsWithDelimiter ? route.slice(0, -delimiter.length) : route) + '(?:' + delimiter + '(?=$))?';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (end) {[m
[32m+[m[32m    route += '$';[m
[32m+[m[32m  } else {[m
[32m+[m[32m    // In non-ending mode, we need the capturing groups to match as much as[m
[32m+[m[32m    // possible by using a positive lookahead to the end or next path segment.[m
[32m+[m[32m    route += strict && endsWithDelimiter ? '' : '(?=' + delimiter + '|$)';[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return attachKeys(new RegExp('^' + route, flags(options)), keys)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Normalize the given path string, returning a regular expression.[m
[32m+[m[32m *[m
[32m+[m[32m * An empty array can be passed in for the keys, which will hold the[m
[32m+[m[32m * placeholder key descriptions. For example, using `/user/:id`, `keys` will[m
[32m+[m[32m * contain `[{ name: 'id', delimiter: '/', optional: false, repeat: false }]`.[m
[32m+[m[32m *[m
[32m+[m[32m * @param  {(string|RegExp|Array)} path[m
[32m+[m[32m * @param  {(Array|Object)=}       keys[m
[32m+[m[32m * @param  {Object=}               options[m
[32m+[m[32m * @return {!RegExp}[m
[32m+[m[32m */[m
[32m+[m[32mfunction pathToRegexp (path, keys, options) {[m
[32m+[m[32m  if (!isarray(keys)) {[m
[32m+[m[32m    options = /** @type {!Object} */ (keys || options);[m
[32m+[m[32m    keys = [];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  options = options || {};[m
[32m+[m
[32m+[m[32m  if (path instanceof RegExp) {[m
[32m+[m[32m    return regexpToRegexp(path, /** @type {!Array} */ (keys))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (isarray(path)) {[m
[32m+[m[32m    return arrayToRegexp(/** @type {!Array} */ (path), /** @type {!Array} */ (keys), options)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return stringToRegexp(/** @type {string} */ (path), /** @type {!Array} */ (keys), options)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mpathToRegexp_1.parse = parse_1;[m
[32m+[m[32mpathToRegexp_1.compile = compile_1;[m
[32m+[m[32mpathToRegexp_1.tokensToFunction = tokensToFunction_1;[m
[32m+[m[32mpathToRegexp_1.tokensToRegExp = tokensToRegExp_1;[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32m// $flow-disable-line[m
[32m+[m[32mvar regexpCompileCache = Object.create(null);[m
[32m+[m
[32m+[m[32mfunction fillParams ([m
[32m+[m[32m  path,[m
[32m+[m[32m  params,[m
[32m+[m[32m  routeMsg[m
[32m+[m[32m) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    var filler =[m
[32m+[m[32m      regexpCompileCache[path] ||[m
[32m+[m[32m      (regexpCompileCache[path] = pathToRegexp_1.compile(path));[m
[32m+[m[32m    return filler(params || {}, { pretty: true })[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m      warn(false, ("missing param for " + routeMsg + ": " + (e.message)));[m
[32m+[m[32m    }[m
[32m+[m[32m    return ''[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mfunction createRouteMap ([m
[32m+[m[32m  routes,[m
[32m+[m[32m  oldPathList,[m
[32m+[m[32m  oldPathMap,[m
[32m+[m[32m  oldNameMap[m
[32m+[m[32m) {[m
[32m+[m[32m  // the path list is used to control path matching priority[m
[32m+[m[32m  var pathList = oldPathList || [];[m
[32m+[m[32m  // $flow-disable-line[m
[32m+[m[32m  var pathMap = oldPathMap || Object.create(null);[m
[32m+[m[32m  // $flow-disable-line[m
[32m+[m[32m  var nameMap = oldNameMap || Object.create(null);[m
[32m+[m
[32m+[m[32m  routes.forEach(function (route) {[m
[32m+[m[32m    addRouteRecord(pathList, pathMap, nameMap, route);[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // ensure wildcard routes are always at the end[m
[32m+[m[32m  for (var i = 0, l = pathList.length; i < l; i++) {[m
[32m+[m[32m    if (pathList[i] === '*') {[m
[32m+[m[32m      pathList.push(pathList.splice(i, 1)[0]);[m
[32m+[m[32m      l--;[m
[32m+[m[32m      i--;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    pathList: pathList,[m
[32m+[m[32m    pathMap: pathMap,[m
[32m+[m[32m    nameMap: nameMap[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction addRouteRecord ([m
[32m+[m[32m  pathList,[m
[32m+[m[32m  pathMap,[m
[32m+[m[32m  nameMap,[m
[32m+[m[32m  route,[m
[32m+[m[32m  parent,[m
[32m+[m[32m  matchAs[m
[32m+[m[32m) {[m
[32m+[m[32m  var path = route.path;[m
[32m+[m[32m  var name = route.name;[m
[32m+[m[32m  if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m    assert(path != null, "\"path\" is required in a route configuration.");[m
[32m+[m[32m    assert([m
[32m+[m[32m      typeof route.component !== 'string',[m
[32m+[m[32m      "route config \"component\" for path: " + (String(path || name)) + " cannot be a " +[m
[32m+[m[32m      "string id. Use an actual component instead."[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var pathToRegexpOptions = route.pathToRegexpOptions || {};[m
[32m+[m[32m  var normalizedPath = normalizePath([m
[32m+[m[32m    path,[m
[32m+[m[32m    parent,[m
[32m+[m[32m    pathToRegexpOptions.strict[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  if (typeof route.caseSensitive === 'boolean') {[m
[32m+[m[32m    pathToRegexpOptions.sensitive = route.caseSensitive;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var record = {[m
[32m+[m[32m    path: normalizedPath,[m
[32m+[m[32m    regex: compileRouteRegex(normalizedPath, pathToRegexpOptions),[m
[32m+[m[32m    components: route.components || { default: route.component },[m
[32m+[m[32m    instances: {},[m
[32m+[m[32m    name: name,[m
[32m+[m[32m    parent: parent,[m
[32m+[m[32m    matchAs: matchAs,[m
[32m+[m[32m    redirect: route.redirect,[m
[32m+[m[32m    beforeEnter: route.beforeEnter,[m
[32m+[m[32m    meta: route.meta || {},[m
[32m+[m[32m    props: route.props == null[m
[32m+[m[32m      ? {}[m
[32m+[m[32m      : route.components[m
[32m+[m[32m        ? route.props[m
[32m+[m[32m        : { default: route.props }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  if (route.children) {[m
[32m+[m[32m    // Warn if route is named, does not redirect and has a default child route.[m
[32m+[m[32m    // If users navigate to this route by name, the default child will[m
[32m+[m[32m    // not be rendered (GH Issue #629)[m
[32m+[m[32m    if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m      if (route.name && !route.redirect && route.children.some(function (child) { return /^\/?$/.test(child.path); })) {[m
[32m+[m[32m        warn([m
[32m+[m[32m          false,[m
[32m+[m[32m          "Named Route '" + (route.name) + "' has a default child route. " +[m
[32m+[m[32m          "When navigating to this named route (:to=\"{name: '" + (route.name) + "'\"), " +[m
[32m+[m[32m          "the default child route will not be rendered. Remove the name from " +[m
[32m+[m[32m          "this route and use the name of the default child route for named " +[m
[32m+[m[32m          "links instead."[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    route.children.forEach(function (child) {[m
[32m+[m[32m      var childMatchAs = matchAs[m
[32m+[m[32m        ? cleanPath((matchAs + "/" + (child.path)))[m
[32m+[m[32m        : undefined;[m
[32m+[m[32m      addRouteRecord(pathList, pathMap, nameMap, child, record, childMatchAs);[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (route.alias !== undefined) {[m
[32m+[m[32m    var aliases = Array.isArray(route.alias)[m
[32m+[m[32m      ? route.alias[m
[32m+[m[32m      : [route.alias];[m
[32m+[m
[32m+[m[32m    aliases.forEach(function (alias) {[m
[32m+[m[32m      var aliasRoute = {[m
[32m+[m[32m        path: alias,[m
[32m+[m[32m        children: route.children[m
[32m+[m[32m      };[m
[32m+[m[32m      addRouteRecord([m
[32m+[m[32m        pathList,[m
[32m+[m[32m        pathMap,[m
[32m+[m[32m        nameMap,[m
[32m+[m[32m        aliasRoute,[m
[32m+[m[32m        parent,[m
[32m+[m[32m        record.path || '/' // matchAs[m
[32m+[m[32m      );[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (!pathMap[record.path]) {[m
[32m+[m[32m    pathList.push(record.path);[m
[32m+[m[32m    pathMap[record.path] = record;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (name) {[m
[32m+[m[32m    if (!nameMap[name]) {[m
[32m+[m[32m      nameMap[name] = record;[m
[32m+[m[32m    } else if (process.env.NODE_ENV !== 'production' && !matchAs) {[m
[32m+[m[32m      warn([m
[32m+[m[32m        false,[m
[32m+[m[32m        "Duplicate named routes definition: " +[m
[32m+[m[32m        "{ name: \"" + name + "\", path: \"" + (record.path) + "\" }"[m
[32m+[m[32m      );[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction compileRouteRegex (path, pathToRegexpOptions) {[m
[32m+[m[32m  var regex = pathToRegexp_1(path, [], pathToRegexpOptions);[m
[32m+[m[32m  if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m    var keys = Object.create(null);[m
[32m+[m[32m    regex.keys.forEach(function (key) {[m
[32m+[m[32m      warn(!keys[key.name], ("Duplicate param keys in route with path: \"" + path + "\""));[m
[32m+[m[32m      keys[key.name] = true;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m[32m  return regex[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction normalizePath (path, parent, strict) {[m
[32m+[m[32m  if (!strict) { path = path.replace(/\/$/, ''); }[m
[32m+[m[32m  if (path[0] === '/') { return path }[m
[32m+[m[32m  if (parent == null) { return path }[m
[32m+[m[32m  return cleanPath(((parent.path) + "/" + path))[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction normalizeLocation ([m
[32m+[m[32m  raw,[m
[32m+[m[32m  current,[m
[32m+[m[32m  append,[m
[32m+[m[32m  router[m
[32m+[m[32m) {[m
[32m+[m[32m  var next = typeof raw === 'string' ? { path: raw } : raw;[m
[32m+[m[32m  // named target[m
[32m+[m[32m  if (next.name || next._normalized) {[m
[32m+[m[32m    return next[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // relative params[m
[32m+[m[32m  if (!next.path && next.params && current) {[m
[32m+[m[32m    next = assign({}, next);[m
[32m+[m[32m    next._normalized = true;[m
[32m+[m[32m    var params = assign(assign({}, current.params), next.params);[m
[32m+[m[32m    if (current.name) {[m
[32m+[m[32m      next.name = current.name;[m
[32m+[m[32m      next.params = params;[m
[32m+[m[32m    } else if (current.matched.length) {[m
[32m+[m[32m      var rawPath = current.matched[current.matched.length - 1].path;[m
[32m+[m[32m      next.path = fillParams(rawPath, params, ("path " + (current.path)));[m
[32m+[m[32m    } else if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m      warn(false, "relative params navigation requires a current route.");[m
[32m+[m[32m    }[m
[32m+[m[32m    return next[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var parsedPath = parsePath(next.path || '');[m
[32m+[m[32m  var basePath = (current && current.path) || '/';[m
[32m+[m[32m  var path = parsedPath.path[m
[32m+[m[32m    ? resolvePath(parsedPath.path, basePath, append || next.append)[m
[32m+[m[32m    : basePath;[m
[32m+[m
[32m+[m[32m  var query = resolveQuery([m
[32m+[m[32m    parsedPath.query,[m
[32m+[m[32m    next.query,[m
[32m+[m[32m    router && router.options.parseQuery[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  var hash = next.hash || parsedPath.hash;[m
[32m+[m[32m  if (hash && hash.charAt(0) !== '#') {[m
[32m+[m[32m    hash = "#" + hash;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    _normalized: true,[m
[32m+[m[32m    path: path,[m
[32m+[m[32m    query: query,[m
[32m+[m[32m    hash: hash[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction assign (a, b) {[m
[32m+[m[32m  for (var key in b) {[m
[32m+[m[32m    a[key] = b[key];[m
[32m+[m[32m  }[m
[32m+[m[32m  return a[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m
[32m+[m[32mfunction createMatcher ([m
[32m+[m[32m  routes,[m
[32m+[m[32m  router[m
[32m+[m[32m) {[m
[32m+[m[32m  var ref = createRouteMap(routes);[m
[32m+[m[32m  var pathList = ref.pathList;[m
[32m+[m[32m  var pathMap = ref.pathMap;[m
[32m+[m[32m  var nameMap = ref.nameMap;[m
[32m+[m
[32m+[m[32m  function addRoutes (routes) {[m
[32m+[m[32m    createRouteMap(routes, pathList, pathMap, nameMap);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function match ([m
[32m+[m[32m    raw,[m
[32m+[m[32m    currentRoute,[m
[32m+[m[32m    redirectedFrom[m
[32m+[m[32m  ) {[m
[32m+[m[32m    var location = normalizeLocation(raw, currentRoute, false, router);[m
[32m+[m[32m    var name = location.name;[m
[32m+[m
[32m+[m[32m    if (name) {[m
[32m+[m[32m      var record = nameMap[name];[m
[32m+[m[32m      if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m        warn(record, ("Route with name '" + name + "' does not exist"));[m
[32m+[m[32m      }[m
[32m+[m[32m      if (!record) { return _createRoute(null, location) }[m
[32m+[m[32m      var paramNames = record.regex.keys[m
[32m+[m[32m        .filter(function (key) { return !key.optional; })[m
[32m+[m[32m        .map(function (key) { return key.name; });[m
[32m+[m
[32m+[m[32m      if (typeof location.params !== 'object') {[m
[32m+[m[32m        location.params = {};[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (currentRoute && typeof currentRoute.params === 'object') {[m
[32m+[m[32m        for (var key in currentRoute.params) {[m
[32m+[m[32m          if (!(key in location.params) && paramNames.indexOf(key) > -1) {[m
[32m+[m[32m            location.params[key] = currentRoute.params[key];[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (record) {[m
[32m+[m[32m        location.path = fillParams(record.path, location.params, ("named route \"" + name + "\""));[m
[32m+[m[32m        return _createRoute(record, location, redirectedFrom)[m
[32m+[m[32m      }[m
[32m+[m[32m    } else if (location.path) {[m
[32m+[m[32m      location.params = {};[m
[32m+[m[32m      for (var i = 0; i < pathList.length; i++) {[m
[32m+[m[32m        var path = pathList[i];[m
[32m+[m[32m        var record$1 = pathMap[path];[m
[32m+[m[32m        if (matchRoute(record$1.regex, location.path, location.params)) {[m
[32m+[m[32m          return _createRoute(record$1, location, redirectedFrom)[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    // no match[m
[32m+[m[32m    return _createRoute(null, location)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function redirect ([m
[32m+[m[32m    record,[m
[32m+[m[32m    location[m
[32m+[m[32m  ) {[m
[32m+[m[32m    var originalRedirect = record.redirect;[m
[32m+[m[32m    var redirect = typeof originalRedirect === 'function'[m
[32m+[m[32m        ? originalRedirect(createRoute(record, location, null, router))[m
[32m+[m[32m        : originalRedirect;[m
[32m+[m
[32m+[m[32m    if (typeof redirect === 'string') {[m
[32m+[m[32m      redirect = { path: redirect };[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!redirect || typeof redirect !== 'object') {[m
[32m+[m[32m      if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m        warn([m
[32m+[m[32m          false, ("invalid redirect option: " + (JSON.stringify(redirect)))[m
[32m+[m[32m        );[m
[32m+[m[32m      }[m
[32m+[m[32m      return _createRoute(null, location)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var re = redirect;[m
[32m+[m[32m    var name = re.name;[m
[32m+[m[32m    var path = re.path;[m
[32m+[m[32m    var query = location.query;[m
[32m+[m[32m    var hash = location.hash;[m
[32m+[m[32m    var params = location.params;[m
[32m+[m[32m    query = re.hasOwnProperty('query') ? re.query : query;[m
[32m+[m[32m    hash = re.hasOwnProperty('hash') ? re.hash : hash;[m
[32m+[m[32m    params = re.hasOwnProperty('params') ? re.params : params;[m
[32m+[m
[32m+[m[32m    if (name) {[m
[32m+[m[32m      // resolved named direct[m
[32m+[m[32m      var targetRecord = nameMap[name];[m
[32m+[m[32m      if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m        assert(targetRecord, ("redirect failed: named route \"" + name + "\" not found."));[m
[32m+[m[32m      }[m
[32m+[m[32m      return match({[m
[32m+[m[32m        _normalized: true,[m
[32m+[m[32m        name: name,[m
[32m+[m[32m        query: query,[m
[32m+[m[32m        hash: hash,[m
[32m+[m[32m        params: params[m
[32m+[m[32m      }, undefined, location)[m
[32m+[m[32m    } else if (path) {[m
[32m+[m[32m      // 1. resolve relative redirect[m
[32m+[m[32m      var rawPath = resolveRecordPath(path, record);[m
[32m+[m[32m      // 2. resolve params[m
[32m+[m[32m      var resolvedPath = fillParams(rawPath, params, ("redirect route with path \"" + rawPath + "\""));[m
[32m+[m[32m      // 3. rematch with existing query and hash[m
[32m+[m[32m      return match({[m
[32m+[m[32m        _normalized: true,[m
[32m+[m[32m        path: resolvedPath,[m
[32m+[m[32m        query: query,[m
[32m+[m[32m        hash: hash[m
[32m+[m[32m      }, undefined, location)[m
[32m+[m[32m    } else {[m
[32m+[m[32m      if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m        warn(false, ("invalid redirect option: " + (JSON.stringify(redirect))));[m
[32m+[m[32m      }[m
[32m+[m[32m      return _createRoute(null, location)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function alias ([m
[32m+[m[32m    record,[m
[32m+[m[32m    location,[m
[32m+[m[32m    matchAs[m
[32m+[m[32m  ) {[m
[32m+[m[32m    var aliasedPath = fillParams(matchAs, location.params, ("aliased route with path \"" + matchAs + "\""));[m
[32m+[m[32m    var aliasedMatch = match({[m
[32m+[m[32m      _normalized: true,[m
[32m+[m[32m      path: aliasedPath[m
[32m+[m[32m    });[m
[32m+[m[32m    if (aliasedMatch) {[m
[32m+[m[32m      var matched = aliasedMatch.matched;[m
[32m+[m[32m      var aliasedRecord = matched[matched.length - 1];[m
[32m+[m[32m      location.params = aliasedMatch.params;[m
[32m+[m[32m      return _createRoute(aliasedRecord, location)[m
[32m+[m[32m    }[m
[32m+[m[32m    return _createRoute(null, location)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function _createRoute ([m
[32m+[m[32m    record,[m
[32m+[m[32m    location,[m
[32m+[m[32m    redirectedFrom[m
[32m+[m[32m  ) {[m
[32m+[m[32m    if (record && record.redirect) {[m
[32m+[m[32m      return redirect(record, redirectedFrom || location)[m
[32m+[m[32m    }[m
[32m+[m[32m    if (record && record.matchAs) {[m
[32m+[m[32m      return alias(record, location, record.matchAs)[m
[32m+[m[32m    }[m
[32m+[m[32m    return createRoute(record, location, redirectedFrom, router)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return {[m
[32m+[m[32m    match: match,[m
[32m+[m[32m    addRoutes: addRoutes[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction matchRoute ([m
[32m+[m[32m  regex,[m
[32m+[m[32m  path,[m
[32m+[m[32m  params[m
[32m+[m[32m) {[m
[32m+[m[32m  var m = path.match(regex);[m
[32m+[m
[32m+[m[32m  if (!m) {[m
[32m+[m[32m    return false[m
[32m+[m[32m  } else if (!params) {[m
[32m+[m[32m    return true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (var i = 1, len = m.length; i < len; ++i) {[m
[32m+[m[32m    var key = regex.keys[i - 1];[m
[32m+[m[32m    var val = typeof m[i] === 'string' ? decodeURIComponent(m[i]) : m[i];[m
[32m+[m[32m    if (key) {[m
[32m+[m[32m      params[key.name] = val;[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return true[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction resolveRecordPath (path, record) {[m
[32m+[m[32m  return resolvePath(path, record.parent ? record.parent.path : '/', true)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m
[32m+[m[32mvar positionStore = Object.create(null);[m
[32m+[m
[32m+[m[32mfunction setupScroll () {[m
[32m+[m[32m  // Fix for #1585 for Firefox[m
[32m+[m[32m  window.history.replaceState({ key: getStateKey() }, '');[m
[32m+[m[32m  window.addEventListener('popstate', function (e) {[m
[32m+[m[32m    saveScrollPosition();[m
[32m+[m[32m    if (e.state && e.state.key) {[m
[32m+[m[32m      setStateKey(e.state.key);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction handleScroll ([m
[32m+[m[32m  router,[m
[32m+[m[32m  to,[m
[32m+[m[32m  from,[m
[32m+[m[32m  isPop[m
[32m+[m[32m) {[m
[32m+[m[32m  if (!router.app) {[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var behavior = router.options.scrollBehavior;[m
[32m+[m[32m  if (!behavior) {[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m    assert(typeof behavior === 'function', "scrollBehavior must be a function");[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // wait until re-render finishes before scrolling[m
[32m+[m[32m  router.app.$nextTick(function () {[m
[32m+[m[32m    var position = getScrollPosition();[m
[32m+[m[32m    var shouldScroll = behavior(to, from, isPop ? position : null);[m
[32m+[m
[32m+[m[32m    if (!shouldScroll) {[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (typeof shouldScroll.then === 'function') {[m
[32m+[m[32m      shouldScroll.then(function (shouldScroll) {[m
[32m+[m[32m        scrollToPosition((shouldScroll), position);[m
[32m+[m[32m      }).catch(function (err) {[m
[32m+[m[32m        if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m          assert(false, err.toString());[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    } else {[m
[32m+[m[32m      scrollToPosition(shouldScroll, position);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction saveScrollPosition () {[m
[32m+[m[32m  var key = getStateKey();[m
[32m+[m[32m  if (key) {[m
[32m+[m[32m    positionStore[key] = {[m
[32m+[m[32m      x: window.pageXOffset,[m
[32m+[m[32m      y: window.pageYOffset[m
[32m+[m[32m    };[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getScrollPosition () {[m
[32m+[m[32m  var key = getStateKey();[m
[32m+[m[32m  if (key) {[m
[32m+[m[32m    return positionStore[key][m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getElementPosition (el, offset) {[m
[32m+[m[32m  var docEl = document.documentElement;[m
[32m+[m[32m  var docRect = docEl.getBoundingClientRect();[m
[32m+[m[32m  var elRect = el.getBoundingClientRect();[m
[32m+[m[32m  return {[m
[32m+[m[32m    x: elRect.left - docRect.left - offset.x,[m
[32m+[m[32m    y: elRect.top - docRect.top - offset.y[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isValidPosition (obj) {[m
[32m+[m[32m  return isNumber(obj.x) || isNumber(obj.y)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction normalizePosition (obj) {[m
[32m+[m[32m  return {[m
[32m+[m[32m    x: isNumber(obj.x) ? obj.x : window.pageXOffset,[m
[32m+[m[32m    y: isNumber(obj.y) ? obj.y : window.pageYOffset[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction normalizeOffset (obj) {[m
[32m+[m[32m  return {[m
[32m+[m[32m    x: isNumber(obj.x) ? obj.x : 0,[m
[32m+[m[32m    y: isNumber(obj.y) ? obj.y : 0[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction isNumber (v) {[m
[32m+[m[32m  return typeof v === 'number'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction scrollToPosition (shouldScroll, position) {[m
[32m+[m[32m  var isObject = typeof shouldScroll === 'object';[m
[32m+[m[32m  if (isObject && typeof shouldScroll.selector === 'string') {[m
[32m+[m[32m    var el = document.querySelector(shouldScroll.selector);[m
[32m+[m[32m    if (el) {[m
[32m+[m[32m      var offset = shouldScroll.offset && typeof shouldScroll.offset === 'object' ? shouldScroll.offset : {};[m
[32m+[m[32m      offset = normalizeOffset(offset);[m
[32m+[m[32m      position = getElementPosition(el, offset);[m
[32m+[m[32m    } else if (isValidPosition(shouldScroll)) {[m
[32m+[m[32m      position = normalizePosition(shouldScroll);[m
[32m+[m[32m    }[m
[32m+[m[32m  } else if (isObject && isValidPosition(shouldScroll)) {[m
[32m+[m[32m    position = normalizePosition(shouldScroll);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (position) {[m
[32m+[m[32m    window.scrollTo(position.x, position.y);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mvar supportsPushState = inBrowser && (function () {[m
[32m+[m[32m  var ua = window.navigator.userAgent;[m
[32m+[m
[32m+[m[32m  if ([m
[32m+[m[32m    (ua.indexOf('Android 2.') !== -1 || ua.indexOf('Android 4.0') !== -1) &&[m
[32m+[m[32m    ua.indexOf('Mobile Safari') !== -1 &&[m
[32m+[m[32m    ua.indexOf('Chrome') === -1 &&[m
[32m+[m[32m    ua.indexOf('Windows Phone') === -1[m
[32m+[m[32m  ) {[m
[32m+[m[32m    return false[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return window.history && 'pushState' in window.history[m
[32m+[m[32m})();[m
[32m+[m
[32m+[m[32m// use User Timing api (if present) for more accurate key precision[m
[32m+[m[32mvar Time = inBrowser && window.performance && window.performance.now[m
[32m+[m[32m  ? window.performance[m
[32m+[m[32m  : Date;[m
[32m+[m
[32m+[m[32mvar _key = genKey();[m
[32m+[m
[32m+[m[32mfunction genKey () {[m
[32m+[m[32m  return Time.now().toFixed(3)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getStateKey () {[m
[32m+[m[32m  return _key[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction setStateKey (key) {[m
[32m+[m[32m  _key = key;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction pushState (url, replace) {[m
[32m+[m[32m  saveScrollPosition();[m
[32m+[m[32m  // try...catch the pushState call to get around Safari[m
[32m+[m[32m  // DOM Exception 18 where it limits to 100 pushState calls[m
[32m+[m[32m  var history = window.history;[m
[32m+[m[32m  try {[m
[32m+[m[32m    if (replace) {[m
[32m+[m[32m      history.replaceState({ key: _key }, '', url);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      _key = genKey();[m
[32m+[m[32m      history.pushState({ key: _key }, '', url);[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (e) {[m
[32m+[m[32m    window.location[replace ? 'replace' : 'assign'](url);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction replaceState (url) {[m
[32m+[m[32m  pushState(url, true);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mfunction runQueue (queue, fn, cb) {[m
[32m+[m[32m  var step = function (index) {[m
[32m+[m[32m    if (index >= queue.length) {[m
[32m+[m[32m      cb();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      if (queue[index]) {[m
[32m+[m[32m        fn(queue[index], function () {[m
[32m+[m[32m          step(index + 1);[m
[32m+[m[32m        });[m
[32m+[m[32m      } else {[m
[32m+[m[32m        step(index + 1);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m[32m  step(0);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mfunction resolveAsyncComponents (matched) {[m
[32m+[m[32m  return function (to, from, next) {[m
[32m+[m[32m    var hasAsync = false;[m
[32m+[m[32m    var pending = 0;[m
[32m+[m[32m    var error = null;[m
[32m+[m
[32m+[m[32m    flatMapComponents(matched, function (def, _, match, key) {[m
[32m+[m[32m      // if it's a function and doesn't have cid attached,[m
[32m+[m[32m      // assume it's an async component resolve function.[m
[32m+[m[32m      // we are not using Vue's default async resolving mechanism because[m
[32m+[m[32m      // we want to halt the navigation until the incoming component has been[m
[32m+[m[32m      // resolved.[m
[32m+[m[32m      if (typeof def === 'function' && def.cid === undefined) {[m
[32m+[m[32m        hasAsync = true;[m
[32m+[m[32m        pending++;[m
[32m+[m
[32m+[m[32m        var resolve = once(function (resolvedDef) {[m
[32m+[m[32m          if (isESModule(resolvedDef)) {[m
[32m+[m[32m            resolvedDef = resolvedDef.default;[m
[32m+[m[32m          }[m
[32m+[m[32m          // save resolved on async factory in case it's used elsewhere[m
[32m+[m[32m          def.resolved = typeof resolvedDef === 'function'[m
[32m+[m[32m            ? resolvedDef[m
[32m+[m[32m            : _Vue.extend(resolvedDef);[m
[32m+[m[32m          match.components[key] = resolvedDef;[m
[32m+[m[32m          pending--;[m
[32m+[m[32m          if (pending <= 0) {[m
[32m+[m[32m            next();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        var reject = once(function (reason) {[m
[32m+[m[32m          var msg = "Failed to resolve async component " + key + ": " + reason;[m
[32m+[m[32m          process.env.NODE_ENV !== 'production' && warn(false, msg);[m
[32m+[m[32m          if (!error) {[m
[32m+[m[32m            error = isError(reason)[m
[32m+[m[32m              ? reason[m
[32m+[m[32m              : new Error(msg);[m
[32m+[m[32m            next(error);[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        var res;[m
[32m+[m[32m        try {[m
[32m+[m[32m          res = def(resolve, reject);[m
[32m+[m[32m        } catch (e) {[m
[32m+[m[32m          reject(e);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (res) {[m
[32m+[m[32m          if (typeof res.then === 'function') {[m
[32m+[m[32m            res.then(resolve, reject);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            // new syntax in Vue 2.3[m
[32m+[m[32m            var comp = res.component;[m
[32m+[m[32m            if (comp && typeof comp.then === 'function') {[m
[32m+[m[32m              comp.then(resolve, reject);[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    if (!hasAsync) { next(); }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction flatMapComponents ([m
[32m+[m[32m  matched,[m
[32m+[m[32m  fn[m
[32m+[m[32m) {[m
[32m+[m[32m  return flatten(matched.map(function (m) {[m
[32m+[m[32m    return Object.keys(m.components).map(function (key) { return fn([m
[32m+[m[32m      m.components[key],[m
[32m+[m[32m      m.instances[key],[m
[32m+[m[32m      m, key[m
[32m+[m[32m    ); })[m
[32m+[m[32m  }))[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction flatten (arr) {[m
[32m+[m[32m  return Array.prototype.concat.apply([], arr)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar hasSymbol =[m
[32m+[m[32m  typeof Symbol === 'function' &&[m
[32m+[m[32m  typeof Symbol.toStringTag === 'symbol';[m
[32m+[m
[32m+[m[32mfunction isESModule (obj) {[m
[32m+[m[32m  return obj.__esModule || (hasSymbol && obj[Symbol.toStringTag] === 'Module')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// in Webpack 2, require.ensure now also returns a Promise[m
[32m+[m[32m// so the resolve/reject functions may get called an extra time[m
[32m+[m[32m// if the user uses an arrow function shorthand that happens to[m
[32m+[m[32m// return that Promise.[m
[32m+[m[32mfunction once (fn) {[m
[32m+[m[32m  var called = false;[m
[32m+[m[32m  return function () {[m
[32m+[m[32m    var args = [], len = arguments.length;[m
[32m+[m[32m    while ( len-- ) args[ len ] = arguments[ len ];[m
[32m+[m
[32m+[m[32m    if (called) { return }[m
[32m+[m[32m    called = true;[m
[32m+[m[32m    return fn.apply(this, args)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mvar History = function History (router, base) {[m
[32m+[m[32m  this.router = router;[m
[32m+[m[32m  this.base = normalizeBase(base);[m
[32m+[m[32m  // start with a route object that stands for "nowhere"[m
[32m+[m[32m  this.current = START;[m
[32m+[m[32m  this.pending = null;[m
[32m+[m[32m  this.ready = false;[m
[32m+[m[32m  this.readyCbs = [];[m
[32m+[m[32m  this.readyErrorCbs = [];[m
[32m+[m[32m  this.errorCbs = [];[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mHistory.prototype.listen = function listen (cb) {[m
[32m+[m[32m  this.cb = cb;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mHistory.prototype.onReady = function onReady (cb, errorCb) {[m
[32m+[m[32m  if (this.ready) {[m
[32m+[m[32m    cb();[m
[32m+[m[32m  } else {[m
[32m+[m[32m    this.readyCbs.push(cb);[m
[32m+[m[32m    if (errorCb) {[m
[32m+[m[32m      this.readyErrorCbs.push(errorCb);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mHistory.prototype.onError = function onError (errorCb) {[m
[32m+[m[32m  this.errorCbs.push(errorCb);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mHistory.prototype.transitionTo = function transitionTo (location, onComplete, onAbort) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m  var route = this.router.match(location, this.current);[m
[32m+[m[32m  this.confirmTransition(route, function () {[m
[32m+[m[32m    this$1.updateRoute(route);[m
[32m+[m[32m    onComplete && onComplete(route);[m
[32m+[m[32m    this$1.ensureURL();[m
[32m+[m
[32m+[m[32m    // fire ready cbs once[m
[32m+[m[32m    if (!this$1.ready) {[m
[32m+[m[32m      this$1.ready = true;[m
[32m+[m[32m      this$1.readyCbs.forEach(function (cb) { cb(route); });[m
[32m+[m[32m    }[m
[32m+[m[32m  }, function (err) {[m
[32m+[m[32m    if (onAbort) {[m
[32m+[m[32m      onAbort(err);[m
[32m+[m[32m    }[m
[32m+[m[32m    if (err && !this$1.ready) {[m
[32m+[m[32m      this$1.ready = true;[m
[32m+[m[32m      this$1.readyErrorCbs.forEach(function (cb) { cb(err); });[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mHistory.prototype.confirmTransition = function confirmTransition (route, onComplete, onAbort) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m  var current = this.current;[m
[32m+[m[32m  var abort = function (err) {[m
[32m+[m[32m    if (isError(err)) {[m
[32m+[m[32m      if (this$1.errorCbs.length) {[m
[32m+[m[32m        this$1.errorCbs.forEach(function (cb) { cb(err); });[m
[32m+[m[32m      } else {[m
[32m+[m[32m        warn(false, 'uncaught error during route navigation:');[m
[32m+[m[32m        console.error(err);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    onAbort && onAbort(err);[m
[32m+[m[32m  };[m
[32m+[m[32m  if ([m
[32m+[m[32m    isSameRoute(route, current) &&[m
[32m+[m[32m    // in the case the route map has been dynamically appended to[m
[32m+[m[32m    route.matched.length === current.matched.length[m
[32m+[m[32m  ) {[m
[32m+[m[32m    this.ensureURL();[m
[32m+[m[32m    return abort()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  var ref = resolveQueue(this.current.matched, route.matched);[m
[32m+[m[32m    var updated = ref.updated;[m
[32m+[m[32m    var deactivated = ref.deactivated;[m
[32m+[m[32m    var activated = ref.activated;[m
[32m+[m
[32m+[m[32m  var queue = [].concat([m
[32m+[m[32m    // in-component leave guards[m
[32m+[m[32m    extractLeaveGuards(deactivated),[m
[32m+[m[32m    // global before hooks[m
[32m+[m[32m    this.router.beforeHooks,[m
[32m+[m[32m    // in-component update hooks[m
[32m+[m[32m    extractUpdateHooks(updated),[m
[32m+[m[32m    // in-config enter guards[m
[32m+[m[32m    activated.map(function (m) { return m.beforeEnter; }),[m
[32m+[m[32m    // async components[m
[32m+[m[32m    resolveAsyncComponents(activated)[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  this.pending = route;[m
[32m+[m[32m  var iterator = function (hook, next) {[m
[32m+[m[32m    if (this$1.pending !== route) {[m
[32m+[m[32m      return abort()[m
[32m+[m[32m    }[m
[32m+[m[32m    try {[m
[32m+[m[32m      hook(route, current, function (to) {[m
[32m+[m[32m        if (to === false || isError(to)) {[m
[32m+[m[32m          // next(false) -> abort navigation, ensure current URL[m
[32m+[m[32m          this$1.ensureURL(true);[m
[32m+[m[32m          abort(to);[m
[32m+[m[32m        } else if ([m
[32m+[m[32m          typeof to === 'string' ||[m
[32m+[m[32m          (typeof to === 'object' && ([m
[32m+[m[32m            typeof to.path === 'string' ||[m
[32m+[m[32m            typeof to.name === 'string'[m
[32m+[m[32m          ))[m
[32m+[m[32m        ) {[m
[32m+[m[32m          // next('/') or next({ path: '/' }) -> redirect[m
[32m+[m[32m          abort();[m
[32m+[m[32m          if (typeof to === 'object' && to.replace) {[m
[32m+[m[32m            this$1.replace(to);[m
[32m+[m[32m          } else {[m
[32m+[m[32m            this$1.push(to);[m
[32m+[m[32m          }[m
[32m+[m[32m        } else {[m
[32m+[m[32m          // confirm transition and pass on the value[m
[32m+[m[32m          next(to);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      abort(e);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  runQueue(queue, iterator, function () {[m
[32m+[m[32m    var postEnterCbs = [];[m
[32m+[m[32m    var isValid = function () { return this$1.current === route; };[m
[32m+[m[32m    // wait until async components are resolved before[m
[32m+[m[32m    // extracting in-component enter guards[m
[32m+[m[32m    var enterGuards = extractEnterGuards(activated, postEnterCbs, isValid);[m
[32m+[m[32m    var queue = enterGuards.concat(this$1.router.resolveHooks);[m
[32m+[m[32m    runQueue(queue, iterator, function () {[m
[32m+[m[32m      if (this$1.pending !== route) {[m
[32m+[m[32m        return abort()[m
[32m+[m[32m      }[m
[32m+[m[32m      this$1.pending = null;[m
[32m+[m[32m      onComplete(route);[m
[32m+[m[32m      if (this$1.router.app) {[m
[32m+[m[32m        this$1.router.app.$nextTick(function () {[m
[32m+[m[32m          postEnterCbs.forEach(function (cb) { cb(); });[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    });[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mHistory.prototype.updateRoute = function updateRoute (route) {[m
[32m+[m[32m  var prev = this.current;[m
[32m+[m[32m  this.current = route;[m
[32m+[m[32m  this.cb && this.cb(route);[m
[32m+[m[32m  this.router.afterHooks.forEach(function (hook) {[m
[32m+[m[32m    hook && hook(route, prev);[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction normalizeBase (base) {[m
[32m+[m[32m  if (!base) {[m
[32m+[m[32m    if (inBrowser) {[m
[32m+[m[32m      // respect <base> tag[m
[32m+[m[32m      var baseEl = document.querySelector('base');[m
[32m+[m[32m      base = (baseEl && baseEl.getAttribute('href')) || '/';[m
[32m+[m[32m      // strip full URL origin[m
[32m+[m[32m      base = base.replace(/^https?:\/\/[^\/]+/, '');[m
[32m+[m[32m    } else {[m
[32m+[m[32m      base = '/';[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  // make sure there's the starting slash[m
[32m+[m[32m  if (base.charAt(0) !== '/') {[m
[32m+[m[32m    base = '/' + base;[m
[32m+[m[32m  }[m
[32m+[m[32m  // remove trailing slash[m
[32m+[m[32m  return base.replace(/\/$/, '')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction resolveQueue ([m
[32m+[m[32m  current,[m
[32m+[m[32m  next[m
[32m+[m[32m) {[m
[32m+[m[32m  var i;[m
[32m+[m[32m  var max = Math.max(current.length, next.length);[m
[32m+[m[32m  for (i = 0; i < max; i++) {[m
[32m+[m[32m    if (current[i] !== next[i]) {[m
[32m+[m[32m      break[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return {[m
[32m+[m[32m    updated: next.slice(0, i),[m
[32m+[m[32m    activated: next.slice(i),[m
[32m+[m[32m    deactivated: current.slice(i)[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction extractGuards ([m
[32m+[m[32m  records,[m
[32m+[m[32m  name,[m
[32m+[m[32m  bind,[m
[32m+[m[32m  reverse[m
[32m+[m[32m) {[m
[32m+[m[32m  var guards = flatMapComponents(records, function (def, instance, match, key) {[m
[32m+[m[32m    var guard = extractGuard(def, name);[m
[32m+[m[32m    if (guard) {[m
[32m+[m[32m      return Array.isArray(guard)[m
[32m+[m[32m        ? guard.map(function (guard) { return bind(guard, instance, match, key); })[m
[32m+[m[32m        : bind(guard, instance, match, key)[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m  return flatten(reverse ? guards.reverse() : guards)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction extractGuard ([m
[32m+[m[32m  def,[m
[32m+[m[32m  key[m
[32m+[m[32m) {[m
[32m+[m[32m  if (typeof def !== 'function') {[m
[32m+[m[32m    // extend now so that global mixins are applied.[m
[32m+[m[32m    def = _Vue.extend(def);[m
[32m+[m[32m  }[m
[32m+[m[32m  return def.options[key][m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction extractLeaveGuards (deactivated) {[m
[32m+[m[32m  return extractGuards(deactivated, 'beforeRouteLeave', bindGuard, true)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction extractUpdateHooks (updated) {[m
[32m+[m[32m  return extractGuards(updated, 'beforeRouteUpdate', bindGuard)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction bindGuard (guard, instance) {[m
[32m+[m[32m  if (instance) {[m
[32m+[m[32m    return function boundRouteGuard () {[m
[32m+[m[32m      return guard.apply(instance, arguments)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction extractEnterGuards ([m
[32m+[m[32m  activated,[m
[32m+[m[32m  cbs,[m
[32m+[m[32m  isValid[m
[32m+[m[32m) {[m
[32m+[m[32m  return extractGuards(activated, 'beforeRouteEnter', function (guard, _, match, key) {[m
[32m+[m[32m    return bindEnterGuard(guard, match, key, cbs, isValid)[m
[32m+[m[32m  })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction bindEnterGuard ([m
[32m+[m[32m  guard,[m
[32m+[m[32m  match,[m
[32m+[m[32m  key,[m
[32m+[m[32m  cbs,[m
[32m+[m[32m  isValid[m
[32m+[m[32m) {[m
[32m+[m[32m  return function routeEnterGuard (to, from, next) {[m
[32m+[m[32m    return guard(to, from, function (cb) {[m
[32m+[m[32m      next(cb);[m
[32m+[m[32m      if (typeof cb === 'function') {[m
[32m+[m[32m        cbs.push(function () {[m
[32m+[m[32m          // #750[m
[32m+[m[32m          // if a router-view is wrapped with an out-in transition,[m
[32m+[m[32m          // the instance may not have been registered at this time.[m
[32m+[m[32m          // we will need to poll for registration until current route[m
[32m+[m[32m          // is no longer valid.[m
[32m+[m[32m          poll(cb, match.instances, key, isValid);[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction poll ([m
[32m+[m[32m  cb, // somehow flow cannot infer this is a function[m
[32m+[m[32m  instances,[m
[32m+[m[32m  key,[m
[32m+[m[32m  isValid[m
[32m+[m[32m) {[m
[32m+[m[32m  if (instances[key]) {[m
[32m+[m[32m    cb(instances[key]);[m
[32m+[m[32m  } else if (isValid()) {[m
[32m+[m[32m    setTimeout(function () {[m
[32m+[m[32m      poll(cb, instances, key, isValid);[m
[32m+[m[32m    }, 16);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m
[32m+[m[32mvar HTML5History = (function (History$$1) {[m
[32m+[m[32m  function HTML5History (router, base) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    History$$1.call(this, router, base);[m
[32m+[m
[32m+[m[32m    var expectScroll = router.options.scrollBehavior;[m
[32m+[m
[32m+[m[32m    if (expectScroll) {[m
[32m+[m[32m      setupScroll();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var initLocation = getLocation(this.base);[m
[32m+[m[32m    window.addEventListener('popstate', function (e) {[m
[32m+[m[32m      var current = this$1.current;[m
[32m+[m
[32m+[m[32m      // Avoiding first `popstate` event dispatched in some browsers but first[m
[32m+[m[32m      // history route not updated since async guard at the same time.[m
[32m+[m[32m      var location = getLocation(this$1.base);[m
[32m+[m[32m      if (this$1.current === START && location === initLocation) {[m
[32m+[m[32m        return[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this$1.transitionTo(location, function (route) {[m
[32m+[m[32m        if (expectScroll) {[m
[32m+[m[32m          handleScroll(router, route, current, true);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if ( History$$1 ) HTML5History.__proto__ = History$$1;[m
[32m+[m[32m  HTML5History.prototype = Object.create( History$$1 && History$$1.prototype );[m
[32m+[m[32m  HTML5History.prototype.constructor = HTML5History;[m
[32m+[m
[32m+[m[32m  HTML5History.prototype.go = function go (n) {[m
[32m+[m[32m    window.history.go(n);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HTML5History.prototype.push = function push (location, onComplete, onAbort) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    var ref = this;[m
[32m+[m[32m    var fromRoute = ref.current;[m
[32m+[m[32m    this.transitionTo(location, function (route) {[m
[32m+[m[32m      pushState(cleanPath(this$1.base + route.fullPath));[m
[32m+[m[32m      handleScroll(this$1.router, route, fromRoute, false);[m
[32m+[m[32m      onComplete && onComplete(route);[m
[32m+[m[32m    }, onAbort);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HTML5History.prototype.replace = function replace (location, onComplete, onAbort) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    var ref = this;[m
[32m+[m[32m    var fromRoute = ref.current;[m
[32m+[m[32m    this.transitionTo(location, function (route) {[m
[32m+[m[32m      replaceState(cleanPath(this$1.base + route.fullPath));[m
[32m+[m[32m      handleScroll(this$1.router, route, fromRoute, false);[m
[32m+[m[32m      onComplete && onComplete(route);[m
[32m+[m[32m    }, onAbort);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HTML5History.prototype.ensureURL = function ensureURL (push) {[m
[32m+[m[32m    if (getLocation(this.base) !== this.current.fullPath) {[m
[32m+[m[32m      var current = cleanPath(this.base + this.current.fullPath);[m
[32m+[m[32m      push ? pushState(current) : replaceState(current);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HTML5History.prototype.getCurrentLocation = function getCurrentLocation () {[m
[32m+[m[32m    return getLocation(this.base)[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return HTML5History;[m
[32m+[m[32m}(History));[m
[32m+[m
[32m+[m[32mfunction getLocation (base) {[m
[32m+[m[32m  var path = window.location.pathname;[m
[32m+[m[32m  if (base && path.indexOf(base) === 0) {[m
[32m+[m[32m    path = path.slice(base.length);[m
[32m+[m[32m  }[m
[32m+[m[32m  return (path || '/') + window.location.search + window.location.hash[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m
[32m+[m[32mvar HashHistory = (function (History$$1) {[m
[32m+[m[32m  function HashHistory (router, base, fallback) {[m
[32m+[m[32m    History$$1.call(this, router, base);[m
[32m+[m[32m    // check history fallback deeplinking[m
[32m+[m[32m    if (fallback && checkFallback(this.base)) {[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m[32m    ensureSlash();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if ( History$$1 ) HashHistory.__proto__ = History$$1;[m
[32m+[m[32m  HashHistory.prototype = Object.create( History$$1 && History$$1.prototype );[m
[32m+[m[32m  HashHistory.prototype.constructor = HashHistory;[m
[32m+[m
[32m+[m[32m  // this is delayed until the app mounts[m
[32m+[m[32m  // to avoid the hashchange listener being fired too early[m
[32m+[m[32m  HashHistory.prototype.setupListeners = function setupListeners () {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    var router = this.router;[m
[32m+[m[32m    var expectScroll = router.options.scrollBehavior;[m
[32m+[m[32m    var supportsScroll = supportsPushState && expectScroll;[m
[32m+[m
[32m+[m[32m    if (supportsScroll) {[m
[32m+[m[32m      setupScroll();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    window.addEventListener(supportsPushState ? 'popstate' : 'hashchange', function () {[m
[32m+[m[32m      var current = this$1.current;[m
[32m+[m[32m      if (!ensureSlash()) {[m
[32m+[m[32m        return[m
[32m+[m[32m      }[m
[32m+[m[32m      this$1.transitionTo(getHash(), function (route) {[m
[32m+[m[32m        if (supportsScroll) {[m
[32m+[m[32m          handleScroll(this$1.router, route, current, true);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (!supportsPushState) {[m
[32m+[m[32m          replaceHash(route.fullPath);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HashHistory.prototype.push = function push (location, onComplete, onAbort) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    var ref = this;[m
[32m+[m[32m    var fromRoute = ref.current;[m
[32m+[m[32m    this.transitionTo(location, function (route) {[m
[32m+[m[32m      pushHash(route.fullPath);[m
[32m+[m[32m      handleScroll(this$1.router, route, fromRoute, false);[m
[32m+[m[32m      onComplete && onComplete(route);[m
[32m+[m[32m    }, onAbort);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HashHistory.prototype.replace = function replace (location, onComplete, onAbort) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    var ref = this;[m
[32m+[m[32m    var fromRoute = ref.current;[m
[32m+[m[32m    this.transitionTo(location, function (route) {[m
[32m+[m[32m      replaceHash(route.fullPath);[m
[32m+[m[32m      handleScroll(this$1.router, route, fromRoute, false);[m
[32m+[m[32m      onComplete && onComplete(route);[m
[32m+[m[32m    }, onAbort);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HashHistory.prototype.go = function go (n) {[m
[32m+[m[32m    window.history.go(n);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HashHistory.prototype.ensureURL = function ensureURL (push) {[m
[32m+[m[32m    var current = this.current.fullPath;[m
[32m+[m[32m    if (getHash() !== current) {[m
[32m+[m[32m      push ? pushHash(current) : replaceHash(current);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  HashHistory.prototype.getCurrentLocation = function getCurrentLocation () {[m
[32m+[m[32m    return getHash()[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return HashHistory;[m
[32m+[m[32m}(History));[m
[32m+[m
[32m+[m[32mfunction checkFallback (base) {[m
[32m+[m[32m  var location = getLocation(base);[m
[32m+[m[32m  if (!/^\/#/.test(location)) {[m
[32m+[m[32m    window.location.replace([m
[32m+[m[32m      cleanPath(base + '/#' + location)[m
[32m+[m[32m    );[m
[32m+[m[32m    return true[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction ensureSlash () {[m
[32m+[m[32m  var path = getHash();[m
[32m+[m[32m  if (path.charAt(0) === '/') {[m
[32m+[m[32m    return true[m
[32m+[m[32m  }[m
[32m+[m[32m  replaceHash('/' + path);[m
[32m+[m[32m  return false[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getHash () {[m
[32m+[m[32m  // We can't use window.location.hash here because it's not[m
[32m+[m[32m  // consistent across browsers - Firefox will pre-decode it![m
[32m+[m[32m  var href = window.location.href;[m
[32m+[m[32m  var index = href.indexOf('#');[m
[32m+[m[32m  return index === -1 ? '' : href.slice(index + 1)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getUrl (path) {[m
[32m+[m[32m  var href = window.location.href;[m
[32m+[m[32m  var i = href.indexOf('#');[m
[32m+[m[32m  var base = i >= 0 ? href.slice(0, i) : href;[m
[32m+[m[32m  return (base + "#" + path)[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction pushHash (path) {[m
[32m+[m[32m  if (supportsPushState) {[m
[32m+[m[32m    pushState(getUrl(path));[m
[32m+[m[32m  } else {[m
[32m+[m[32m    window.location.hash = path;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction replaceHash (path) {[m
[32m+[m[32m  if (supportsPushState) {[m
[32m+[m[32m    replaceState(getUrl(path));[m
[32m+[m[32m  } else {[m
[32m+[m[32m    window.location.replace(getUrl(path));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m
[32m+[m[32mvar AbstractHistory = (function (History$$1) {[m
[32m+[m[32m  function AbstractHistory (router, base) {[m
[32m+[m[32m    History$$1.call(this, router, base);[m
[32m+[m[32m    this.stack = [];[m
[32m+[m[32m    this.index = -1;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if ( History$$1 ) AbstractHistory.__proto__ = History$$1;[m
[32m+[m[32m  AbstractHistory.prototype = Object.create( History$$1 && History$$1.prototype );[m
[32m+[m[32m  AbstractHistory.prototype.constructor = AbstractHistory;[m
[32m+[m
[32m+[m[32m  AbstractHistory.prototype.push = function push (location, onComplete, onAbort) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    this.transitionTo(location, function (route) {[m
[32m+[m[32m      this$1.stack = this$1.stack.slice(0, this$1.index + 1).concat(route);[m
[32m+[m[32m      this$1.index++;[m
[32m+[m[32m      onComplete && onComplete(route);[m
[32m+[m[32m    }, onAbort);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AbstractHistory.prototype.replace = function replace (location, onComplete, onAbort) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    this.transitionTo(location, function (route) {[m
[32m+[m[32m      this$1.stack = this$1.stack.slice(0, this$1.index).concat(route);[m
[32m+[m[32m      onComplete && onComplete(route);[m
[32m+[m[32m    }, onAbort);[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AbstractHistory.prototype.go = function go (n) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m    var targetIndex = this.index + n;[m
[32m+[m[32m    if (targetIndex < 0 || targetIndex >= this.stack.length) {[m
[32m+[m[32m      return[m
[32m+[m[32m    }[m
[32m+[m[32m    var route = this.stack[targetIndex];[m
[32m+[m[32m    this.confirmTransition(route, function () {[m
[32m+[m[32m      this$1.index = targetIndex;[m
[32m+[m[32m      this$1.updateRoute(route);[m
[32m+[m[32m    });[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AbstractHistory.prototype.getCurrentLocation = function getCurrentLocation () {[m
[32m+[m[32m    var current = this.stack[this.stack.length - 1];[m
[32m+[m[32m    return current ? current.fullPath : '/'[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  AbstractHistory.prototype.ensureURL = function ensureURL () {[m
[32m+[m[32m    // noop[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  return AbstractHistory;[m
[32m+[m[32m}(History));[m
[32m+[m
[32m+[m[32m/*  */[m
[32m+[m
[32m+[m[32mvar VueRouter = function VueRouter (options) {[m
[32m+[m[32m  if ( options === void 0 ) options = {};[m
[32m+[m
[32m+[m[32m  this.app = null;[m
[32m+[m[32m  this.apps = [];[m
[32m+[m[32m  this.options = options;[m
[32m+[m[32m  this.beforeHooks = [];[m
[32m+[m[32m  this.resolveHooks = [];[m
[32m+[m[32m  this.afterHooks = [];[m
[32m+[m[32m  this.matcher = createMatcher(options.routes || [], this);[m
[32m+[m
[32m+[m[32m  var mode = options.mode || 'hash';[m
[32m+[m[32m  this.fallback = mode === 'history' && !supportsPushState && options.fallback !== false;[m
[32m+[m[32m  if (this.fallback) {[m
[32m+[m[32m    mode = 'hash';[m
[32m+[m[32m  }[m
[32m+[m[32m  if (!inBrowser) {[m
[32m+[m[32m    mode = 'abstract';[m
[32m+[m[32m  }[m
[32m+[m[32m  this.mode = mode;[m
[32m+[m
[32m+[m[32m  switch (mode) {[m
[32m+[m[32m    case 'history':[m
[32m+[m[32m      this.history = new HTML5History(this, options.base);[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'hash':[m
[32m+[m[32m      this.history = new HashHistory(this, options.base, this.fallback);[m
[32m+[m[32m      break[m
[32m+[m[32m    case 'abstract':[m
[32m+[m[32m      this.history = new AbstractHistory(this, options.base);[m
[32m+[m[32m      break[m
[32m+[m[32m    default:[m
[32m+[m[32m      if (process.env.NODE_ENV !== 'production') {[m
[32m+[m[32m        assert(false, ("invalid mode: " + mode));[m
[32m+[m[32m      }[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar prototypeAccessors = { currentRoute: { configurable: true } };[m
[32m+[m
[32m+[m[32mVueRouter.prototype.match = function match ([m
[32m+[m[32m  raw,[m
[32m+[m[32m  current,[m
[32m+[m[32m  redirectedFrom[m
[32m+[m[32m) {[m
[32m+[m[32m  return this.matcher.match(raw, current, redirectedFrom)[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mprototypeAccessors.currentRoute.get = function () {[m
[32m+[m[32m  return this.history && this.history.current[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.init = function init (app /* Vue component instance */) {[m
[32m+[m[32m    var this$1 = this;[m
[32m+[m
[32m+[m[32m  process.env.NODE_ENV !== 'production' && assert([m
[32m+[m[32m    install.installed,[m
[32m+[m[32m    "not installed. Make sure to call `Vue.use(VueRouter)` " +[m
[32m+[m[32m    "before creating root instance."[m
[32m+[m[32m  );[m
[32m+[m
[32m+[m[32m  this.apps.push(app);[m
[32m+[m
[32m+[m[32m  // main app already initialized.[m
[32m+[m[32m  if (this.app) {[m
[32m+[m[32m    return[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  this.app = app;[m
[32m+[m
[32m+[m[32m  var history = this.history;[m
[32m+[m
[32m+[m[32m  if (history instanceof HTML5History) {[m
[32m+[m[32m    history.transitionTo(history.getCurrentLocation());[m
[32m+[m[32m  } else if (history instanceof HashHistory) {[m
[32m+[m[32m    var setupHashListener = function () {[m
[32m+[m[32m      history.setupListeners();[m
[32m+[m[32m    };[m
[32m+[m[32m    history.transitionTo([m
[32m+[m[32m      history.getCurrentLocation(),[m
[32m+[m[32m      setupHashListener,[m
[32m+[m[32m      setupHashListener[m
[32m+[m[32m    );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  history.listen(function (route) {[m
[32m+[m[32m    this$1.apps.forEach(function (app) {[m
[32m+[m[32m      app._route = route;[m
[32m+[m[32m    });[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.beforeEach = function beforeEach (fn) {[m
[32m+[m[32m  return registerHook(this.beforeHooks, fn)[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.beforeResolve = function beforeResolve (fn) {[m
[32m+[m[32m  return registerHook(this.resolveHooks, fn)[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.afterEach = function afterEach (fn) {[m
[32m+[m[32m  return registerHook(this.afterHooks, fn)[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.onReady = function onReady (cb, errorCb) {[m
[32m+[m[32m  this.history.onReady(cb, errorCb);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.onError = function onError (errorCb) {[m
[32m+[m[32m  this.history.onError(errorCb);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.push = function push (location, onComplete, onAbort) {[m
[32m+[m[32m  this.history.push(location, onComplete, onAbort);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.replace = function replace (location, onComplete, onAbort) {[m
[32m+[m[32m  this.history.replace(location, onComplete, onAbort);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.go = function go (n) {[m
[32m+[m[32m  this.history.go(n);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.back = function back () {[m
[32m+[m[32m  this.go(-1);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.forward = function forward () {[m
[32m+[m[32m  this.go(1);[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.getMatchedComponents = function getMatchedComponents (to) {[m
[32m+[m[32m  var route = to[m
[32m+[m[32m    ? to.matched[m
[32m+[m[32m      ? to[m
[32m+[m[32m      : this.resolve(to).route[m
[32m+[m[32m    : this.currentRoute;[m
[32m+[m[32m  if (!route) {[m
[32m+[m[32m    return [][m
[32m+[m[32m  }[m
[32m+[m[32m  return [].concat.apply([], route.matched.map(function (m) {[m
[32m+[m[32m    return Object.keys(m.components).map(function (key) {[m
[32m+[m[32m      return m.components[key][m
[32m+[m[32m    })[m
[32m+[m[32m  }))[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.resolve = function resolve ([m
[32m+[m[32m  to,[m
[32m+[m[32m  current,[m
[32m+[m[32m  append[m
[32m+[m[32m) {[m
[32m+[m[32m  var location = normalizeLocation([m
[32m+[m[32m    to,[m
[32m+[m[32m    current || this.history.current,[m
[32m+[m[32m    append,[m
[32m+[m[32m    this[m
[32m+[m[32m  );[m
[32m+[m[32m  var route = this.match(location, current);[m
[32m+[m[32m  var fullPath = route.redirectedFrom || route.fullPath;[m
[32m+[m[32m  var base = this.history.base;[m
[32m+[m[32m  var href = createHref(base, fullPath, this.mode);[m
[32m+[m[32m  return {[m
[32m+[m[32m    location: location,[m
[32m+[m[32m    route: route,[m
[32m+[m[32m    href: href,[m
[32m+[m[32m    // for backwards compat[m
[32m+[m[32m    normalizedTo: location,[m
[32m+[m[32m    resolved: route[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mVueRouter.prototype.addRoutes = function addRoutes (routes) {[m
[32m+[m[32m  this.matcher.addRoutes(routes);[m
[32m+[m[32m  if (this.history.current !== START) {[m
[32m+[m[32m    this.history.transitionTo(this.history.getCurrentLocation());[m
[32m+[m[32m  }[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mObject.defineProperties( VueRouter.prototype, prototypeAccessors );[m
[32m+[m
[32m+[m[32mfunction registerHook (list, fn) {[m
[32m+[m[32m  list.push(fn);[m
[32m+[m[32m  return function () {[m
[32m+[m[32m    var i = list.indexOf(fn);[m
[32m+[m[32m    if (i > -1) { list.splice(i, 1); }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction createHref (base, fullPath, mode) {[m
[32m+[m[32m  var path = mode === 'hash' ? '#' + fullPath : fullPath;[m
[32m+[m[32m  return base ? cleanPath(base + '/' + path) : path[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mVueRouter.install = install;[m
[32m+[m[32mVueRouter.version = '3.0.1';[m
[32m+[m
[32m+[m[32mif (inBrowser && window.Vue) {[m
[32m+[m[32m  window.Vue.use(VueRouter);[m
[32m+[m[32m}[m
